{
    "manifest_version": 3,
    "name": "Fiverr Contect Info Extractor",
    "version": "1.0.0",
    "description": "Extract email addresses from all open tabs.",
    "permissions": [
        "tabs",
        "scripting",
        "clipboardWrite"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_popup": "popup.html"
    }
}