{
    "manifest_version": 3,
    "name": "Fiverr Contect Info Extractor",
    "version": "1.0.0",
    "description": "Extract email addresses from all open tabs.",
    "icons": {
        "16": "assets/icons/icon-16.png",
        "32": "assets/icons/icon-32.png",
        "48": "assets/icons/icon-48.png",
        "128": "assets/icons/icon-128.png"
    },
    "permissions": [
        "tabs",
        "scripting",
        "clipboardWrite"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_popup": "popup/popup.html",
        "default_icon": {
            "16": "assets/icons/icon-16.png",
            "32": "assets/icons/icon-32.png",
            "48": "assets/icons/icon-48.png",
            "128": "assets/icons/icon-128.png"
        }
    }
}