{
  "manifest_version": 3,
  "name": "Fiverr Navigator",
  "version": "1.0.0",
  "description": "Navigate Fiverr contact list with Page Up/Page Down keys",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://pro.fiverr.com/inbox",
        "https://pro.fiverr.com/inbox/*"
      ],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}

